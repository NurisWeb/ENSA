        xmlns="http://www.w3.org/2000/svg"
        xmlns:xlink="http://www.w3.org/1999/xlink"
        width="120"
        zoomAndPan="magnify"
        viewBox="0 0 375 172.499995"
        height="60"
        preserveAspectRatio="xMidYMid meet"
        version="1.0"
        ><defs
          ><clipPath id="06bd60762b"
            ><path
              d="M 0.542969 0 L 374.457031 0 L 374.457031 172 L 0.542969 172 Z M 0.542969 0 "
              clip-rule="nonzero"></path></clipPath
          ><image
            x="0"
            y="0"
            width="927"
            xlink:href="data:image/jpeg;base64,"
            id="23ca753b73"
            height="415"
            preserveAspectRatio="xMidYMid meet"
          /></defs
        ><g clip-path="url(#06bd60762b)"
          ><g transform="matrix(0.424336, 0, 0, 0.423471, -3.227785, -3.953551)"
            ><image
              x="0"
              y="0"
              width="927"
              xlink:href="data:image/jpeg;base64,"
              height="415"
              preserveAspectRatio="xMidYMid meet"
            /></g
          ></g
        ></svg
      >
    </a>
    <button
      data-collapse-toggle="navbar-multi-level"
      type="button"
      class="inline-flex items-center p-2 w-10 h-10 justify-center text-gray-500 rounded-lg md:hidden hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 dark:hover:bg-gray-700 dark:focus:ring-gray-600"
      aria-controls="navbar-multi-levelLeistungen"
      aria-expanded="false"
    >
      <span class="sr-only">Open main menu</span>
      <svg
        class="w-5 h-5"
        aria-hidden="true"
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 17 14"
      >
        <path
          stroke="currentColor"
          stroke-linecap="round"
          stroke-linejoin="round"
          stroke-width="2"
          d="M1 1h15M1 7h15M1 13h15"></path>
      </svg>
    </button>
    <div class="hidden w-full md:block md:w-auto" id="navbar-multi-level">
      <ul
        class="flex flex-col font-medium p-4 md:p-0 mt-4 border border-gray-100 rounded-lg bg-gray-50 md:space-x-8 rtl:space-x-reverse md:flex-row md:mt-0 md:border-0 md:bg-white"
      >
        <li>
          <a
            href="/"
            class="nav-linkk font-bold uppercase block py-2 px-3 text-black bg-blue-700 text-lg rounded md:bg-transparent md:text-blue-700 md:p-0"
            data-active-page="start">Start</a
          >
        </li>

        <li>
          <a
            href="/leistungen"
            class="nav-linkk font-bold uppercase text-lg block py-2 px-3 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0"
            data-active-page="RFprei"
          >
            Leistungen</a
          >
        </li>
        <li>
          <a
            href="/regionen"
            class="nav-linkk font-bold uppercase text-lg block py-2 px-3 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0"
            data-active-page="preise">Regionen</a
          >
        </li>

        <li>
          <a
            href="/ueber-uns"
            class="nav-linkk font-bold uppercase text-lg block py-2 px-3 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0"
            data-active-page="preise">Über uns</a
          >
        </li>
        <li>
          <button
            id="dropdownNavbarLink"
            data-dropdown-toggle="dropdownNavbar"
            class="nav-linkk font-bold uppercase text-lg flex items-center justify-between w-full py-2 px-3 text-gray-900 hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0"
            data-active-page="informationen"
          >
            Informationen <svg
              class="w-2.5 h-2.5 ms-2.5"
              aria-hidden="true"
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 10 6"
            >
              <path
                stroke="currentColor"
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="m1 1 4 4 4-4"></path>
            </svg>
          </button>

          <!-- Dropdown menu -->
          <div
            id="dropdownNavbar"
            class="text-lg z-10 hidden font-normal bg-white divide-y divide-gray-100 rounded-lg shadow w-44"
          >
            <ul
              class="py-2 text-gray-700 dark:text-gray-200"
              aria-labelledby="dropdownLargeButton"
            >
              <li>
                <a
                  href="/allergiker"
                  class="font-bold uppercase text-lg block pl-4 px-2 py-2 hover:bg-gray-100"
                  >Allergiker</a
                >
              </li>
              <li>
                <a
                  href="/professionelle-teppichreinigung"
                  class="font-bold uppercase text-lg block px-2 pl-4 py-2 hover:bg-gray-100"
                  >Professionelle Teppichreinigung</a
                >
              </li>
              <li>
                <a
                  href="/polstermoebel"
                  class="font-bold uppercase text-lg block px-2 py-2 pl-4 hover:bg-gray-100"
                  >Polstermöbel</a
                >
              </li>
              <li>
                <a
                  href="/teppicharten"
                  class="font-bold uppercase text-lg block px-2 py-2 pl-4 hover:bg-gray-100"
                  >Teppicharten</a
                >
              </li>
              <li>
                <a
                  href="/teppichboden"
                  class="font-bold uppercase text-lg pl-4 block px-2 py-2 hover:bg-gray-100"
                  >Teppichboden</a
                >
              </li>
              <li>
                <a
                  href="/wolltepiche"
                  class="font-bold uppercase text-lg block pl-4 px-2 py-2 hover:bg-gray-100"
                  >Wollteppiche</a
                >
              </li>
              <li>
                <a
                  href="/teppich-milben"
                  class="font-bold uppercase text-lg block px-2 pl-4 py-2 hover:bg-gray-100"
                  >Teppich Milben</a
                >
              </li>
              <li>
                <a
                  href="/seidenteppich"
                  class="font-bold uppercase text-lg block px-2 py-2 pl-4 hover:bg-gray-100"
                  >Seidenteppich</a
                >
              </li>
              <li>
                <a
                  href="/nepaltepich"
                  class="font-bold uppercase text-lg pl-4 block px-2 py-2 hover:bg-gray-100"
                  >Nepaltepich</a
                >
              </li>
              <li>
                <a
                  href="/hochflorteppiche"
                  class="font-bold uppercase text-lg pl-4 block px-2 py-2 hover:bg-gray-100"
                  >Hochflorteppiche</a
                >
              </li>
            </ul>
          </div>
        </li>
      </ul>
    </div>
  </div>
</nav>
<button
  id="scrollToTopBtn"
  class="hidden fixed bottom-4 right-4 bg-teal-400 text-white p-3 rounded-full"
>
  <svg class="w-8 h-8 text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="none" viewBox="0 0 24 24">
  <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6v13m0-13 4 4m-4-4-4 4"/>
</svg>

</button>

<style>
  #scrollToTopBtn {
    transition: opacity 0.3s ease-in-out;
  }

  #scrollToTopBtn.visible {
    opacity: 1;
  }

  #scrollToTopBtn.hidden {
    opacity: 0;
  }

  nav {
    box-shadow: 0 4px 6px 0 rgba(0, 0, 0, 0.5);
  }
  body {
    font-family: "Montserrat Variable", sans-serif;
  }
  .max-w-screen {
    max-width: 80%;
  }
  /* 
  li {
    padding: 2px 11px;
    transition: 0.371s ease-in-out;
  } */

  /*   li:hover {
    background-color: aliceblue;
    border-radius: 8px;
  } */

  @media (width <= 1550px) {
    .text-lg {
      font-size: 1.05rem !important;
    }
  }

  @media (width <= 1300px) {
    .max-w-screen {
      max-width: 96%;
    }
  }

  @media (width <= 1200px) {
    .text-lg {
      font-size: 0.85rem !important;
    }

    image {
      width: 650px;
    }
  }

  @media (width <= 1000px) {
    .max-w-screen {
      max-width: 97%;
    }
  }

  /* Standard-Link-Stil */
  .nav-linkk {
    color: #000; /* Schwarzer Text */
  }

  /* Aktiver Link-Stil */
  .nav-linkk.active {
    color: #1d4ed8; /* Tailwind's blue-700 Farbe */
  }

  #dropdownNavbarLeistungen {
    z-index: 200;
  }

  @media (width <= 767px) {
    .p-2 {
      padding: 0.1rem;
    }
  }

  #dropdownNavbar {
    width: 230px;
    z-index: 200 !important;
  }

  /* Verstecke den Header initial */
  /* Initiales Styling des Headers für Desktop */
  @media (min-width: 768px) {
    nav {
      transition:
        transform 0.3s ease-in-out,
        opacity 0.3s ease-in-out;
      transform: translateY(0); /* Header initial sichtbar */
      opacity: 1; /* Header vollständig sichtbar */
      position: fixed;
      top: 0;
      width: 100%;
      z-index: 999;
    }
    /* Verstecke den Header mit fade-out Effekt */
    nav.hidden {
      transform: translateY(-100%);
      opacity: 0; /* Header wird unsichtbar */
    }

    /* Zeige den Header mit fade-in Effekt */
    nav.visible {
      transform: translateY(0);
      opacity: 1; /* Header wird sichtbar */
    }

    /* Scroll-to-Top Button */
    #scrollToTopBtn {
      transition: opacity 0.3s ease-in-out;
    }

    #scrollToTopBtn.visible {
      opacity: 1;
      pointer-events: all;
    }

    #scrollToTopBtn.hidden {
      opacity: 0;
      pointer-events: none;
    }
  }

  /* Auf mobilen Ansichten ist der Header immer fixiert */
</style>

<script>
  // Funktion, um den aktiven Menüpunkt basierend auf dem aktuellen Pfad festzulegen
  import { initFlowbite } from "flowbite";

  function setActiveNavLink() {
    const navLinks = document.querySelectorAll(".nav-linkk");
    const currentPath = window.location.pathname;

    navLinks.forEach((link) => {
      const page = link.getAttribute("data-active-page");

      if (
        (page === "start" && currentPath === "/") ||
        currentPath.includes("/" + page + "/")
      ) {
        link.classList.add("active");
      } else {
        link.classList.remove("active");
      }
      if (
        (page === "matratzen" && currentPath === "/matratzen") ||
        currentPath.includes("/matratzen" + page + "/matratzen")
      ) {
        link.classList.add("active");
      } else {
        link.classList.remove("active");
      }
    });
  }

  // Funktion zur Initialisierung der Dropdown-Menüs
  function initDropdownMenus() {
    // Wähle alle Dropdown-Toggles aus
    const dropdownToggles = document.querySelectorAll("[data-dropdown-toggle]");

    dropdownToggles.forEach((toggle) => {
      const dropdownId = toggle.getAttribute("data-dropdown-toggle");
      const dropdownMenu = document.getElementById(dropdownId!);

      // Entferne vorherige Event Listener, um Duplikate zu vermeiden
      toggle.removeEventListener("click", toggleDropdown);
      toggle.addEventListener("click", toggleDropdown);

      function toggleDropdown(event) {
        event.preventDefault();
        dropdownMenu!.classList.toggle("hidden");
      }
    });
  }

  // Funktionen beim initialen Laden der Seite aufrufen
  setActiveNavLink();
  initDropdownMenus();

  // Event Listener für Routenwechsel hinzufügen
  document.addEventListener("astro:after-swap", () => {
    setActiveNavLink();
    initDropdownMenus();
    initFlowbite();
  });

  /*  */
  const scrollToTopBtn = document.getElementById("scrollToTopBtn");

  if (window.scrollY > 200) {
    scrollToTopBtn!.classList.add("visible");
    scrollToTopBtn!.classList.remove("hidden");
  } else {
    scrollToTopBtn!.classList.remove("visible");
    scrollToTopBtn!.classList.add("hidden");
  }

  // Event Listener für den Scroll-to-Top Button
  scrollToTopBtn!.addEventListener("click", () => {
    window.scrollTo({ top: 0, behavior: "smooth" });
  });
</script>
